{
    "name": "fwk/di",
    "description": "Dependency Injection Container for Happy Developers",
    "type": "library",
    "keywords": ["dependency", "di", "dependency-injection", "container", "services", "happy-developer"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Julien",
            "email": "julien@nitronet.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "fwk/xml": "dev-master",
        "fwk/events": "dev-master",
        "container-interop/container-interop": "1.*"
    },
    "require-dev": {
        "zendframework/zend-stdlib": "dev-master",
        "ocramius/proxy-manager": "1.0.*"
    },
    "suggest": {
        "ocramius/proxy-manager": "Allows usage of Lazy Services Loading (ProxyManager)"
    },
    "autoload": {
        "psr-4": {
            "Fwk\\Di\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}